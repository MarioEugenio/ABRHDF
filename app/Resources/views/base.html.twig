<!DOCTYPE html>
<html ng-app="COLIH">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{% block title %}ABMRHDF{% endblock %}</title>

    <link href="{{ asset('bundles/corebase/css/bootstrap.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/corebase/css/bootstrap-switch.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/corebase/bower_components/metisMenu/dist/metisMenu.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/corebase/dist/css/sb-admin-2.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/corebase/bower_components/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/corebase/js/external/jquery/jquery-ui-1.11.4.custom/jquery-ui.css') }}" rel="stylesheet" type="text/css" />

    {% block stylesheets %}{% endblock %}

</head>
<body>
{% block body %}{% endblock %}

{# Componentes Jquery #}
<script src="{{ asset('bundles/corebase/js/external/jquery/jquery.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/jquery/jquery-maskmoney.js') }}"></script>

<script src="{{ asset('bundles/corebase/js/external/bootstrap/bootstrap.min.js') }}"></script>

{# fosjsrouting #}
<script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
<script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

{# Componentes angular / Diretivas #}
<script src="{{ asset('bundles/corebase/js/external/angular/angular.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/angular/angular-router.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/angular/angular-validation.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/angular/angular-maskmoney.js') }}"></script>

<script src="{{ asset('bundles/corebase/js/external/angular/directive/strap/angular-strap.min.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/angular/directive/strap/angular-strap.tpl.min.js') }}"></script>

<script src="{{ asset('bundles/corebase/js/external/angular/directive/switch/angular-bootstrap-switch.min.js') }}"></script>

<script src="{{ asset('bundles/corebase/bower_components/metisMenu/dist/metisMenu.min.js') }}"></script>
<script src="{{ asset('bundles/corebase/dist/js/sb-admin-2.js') }}"></script>

<script src="{{ asset('bundles/corebase/js/external/jquery/bootstrap-switch.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/jquery/jquery-ui-1.11.4.custom/jquery-ui.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/jquery/jquery-ui-1.11.4.custom/ui/i18n/datepicker-pt-BR.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/angular/ui-utils-0.2.3/ui-utils.js') }}"></script>
<script src="{{ asset('bundles/corebase/js/external/angular/date.js') }}"></script>

{# Componentes SNC core #}
<script src="{{ asset('bundles/corebase/js/Core.js') }}"></script>

{% block javascripts %}{% endblock %}
</body>
</html>
